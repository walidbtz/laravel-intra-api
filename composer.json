{
    "name": "walidbtz/laravel-auth-intra",
    "description": "A Laravel package to interact with the 42 Intra api ",
    "license": "Apache-2.0",
    "version": "1",
    "authors": [
        {
            "author": "walidbtz",
            "email": "walidboutzougarte@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "auth",
	"intra"
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^8.0|^9.0",
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Walidbtz\\IntraApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Walidbtz\\IntraApi\\Tests\\": "tests",
            "Walidbtz\\IntraApi\\Database\\": "database"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Walidbtz\\IntraApi\\IntraServiceProvider"
            ],
            "aliases": {
                "IntraAPI": "Walidbtz\\IntraApi\\Facades\\IntraAPI"
            }
        }
    }
}

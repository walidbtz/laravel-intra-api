{
    "name": "naviisml/laravel-intra-api",
    "description": "A Laravel package to interact with the 42 Intra api ",
    "license": "Apache-2.0",
    "version": "1.1.0",
    "authors": [
        {
            "author": "Navi",
            "email": "contact@navel.dev"
        }
    ],
    "keywords": [
        "laravel",
        "intra",
		"api"
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^8.0|^9.0",
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Naviisml\\IntraApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naviisml\\IntraApi\\Tests\\": "tests",
            "Naviisml\\IntraApi\\Database\\": "database"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naviisml\\IntraApi\\IntraServiceProvider"
            ],
            "aliases": {
                "IntraAPI": "Naviisml\\IntraApi\\Facades\\IntraAPI"
            }
        }
    }
}
